// A cpp program to generate encrypted password by shifting each character by 2 ASCII characters
#include <iostream>
using namespace std;
int main() 
{
    char str[100],newstr[100],a;
    int n;
    cout<<"Enter password: ";
    cin>>str;
    for(int i=0;i<10;i++)
    {
        if(str[i]!='\0')
        {
            n=str[i];
            a=n+2;
            newstr[i]=a;
        }
    }
    cout<<"Your encrypted password is: ";
    for(int j=0;j<10;j++)
    {
        if(newstr[j]!='\0')
            cout<<newstr[j];
    }
}
