//Program to claculate  area using constructors
#include <iostream>
using namespace std;
class Area
{
    private:
        float ar;
    public:
        Area(float l,float b)
        {
            ar=l*b;
        }
        float return_area()
        {
            return ar;
        }
};
int main() {
    float l,b,ans;
    cin>>l;
    cin>>b;
    Area rect(l,b);
    ans=rect.return_area();
    cout<<"The area of rectangle = "<<ans<<endl;
    return 0;
}
