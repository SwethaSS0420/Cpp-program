//Program to find minimum no of notes for the given money.
#include <iostream>
using namespace std;
int main()
{
    int d[]={2000,500,100,50,20,10,5,2,1};
    int x,c=0;
    cin>>x;
    for(int i=0;i<9;i++)
    {
        if(d[i]<=x)
        {
            while(x>=d[i])
            {
                x-=d[i];
                cout<<d[i]<<" ";
            }
        }
        if(x==0)
        {
            break;
        }
    }
}
